<div class="container">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Booking ID</th>
        <th>Start Time</th>
        <th>Finish Time</th>
        <th>User ID</th>
        <th>Aircraft ID</th>
        <th class="text-center" colspan="2"> Accion  </th>
      </tr>
    </thead>
    <tbody>
      <% @bookings.each do |b| %>
        <tr>
          <td><%= b.id%></td>
          <td><%= b.start_time%></td>
          <td><%= b.finish_time%></td>
          <td><%= b.user_id %></td>
          <td><%= b.aircraft_id%></td>
          <td><%= link_to 'Show' %></td>
          <td><%= link_to 'Edit' %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
